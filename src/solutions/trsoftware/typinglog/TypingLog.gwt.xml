<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
    "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="TypingLog">
  <!-- This module contains the TypingLogReplayPlayer UI -->
  <inherits name="solutions.trsoftware.commons.Commons"/>
  <inherits name="solutions.trsoftware.gcharts.GoogleCharts"/>

  <source path="client"/>
  <stylesheet src="TypingLog.css"/>

  <entry-point class='solutions.trsoftware.typinglog.client.TypingLogEntryPoint'/>

  <!-- disable CssResource obfuscation (we want to use the original class names): -->
  <set-configuration-property name="CssResource.style" value="stable-notype"/>
  <set-property name="ClientBundle.enableInlining" value="false" />

</module>